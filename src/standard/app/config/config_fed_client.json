{
  "format_version": 2,
  "project_id": "",
  "query": "SELECT * FROM Table;",
  "local_rounds": 1,
  "executors": [
    {
      "tasks": ["init_training", "post_validation"],
      "executor": {
        "path": "flip.nvflare.components.CleanupImages",
        "args": {}
      }
    },
    {
      "tasks": ["train", "submit_model"],
      "executor": {
        "path": "flip.nvflare.executors.RUN_TRAINER",
        "args": {
          "project_id": "{project_id}",
          "query": "{query}"
        }
      }
    },
    {
      "tasks": ["validate"],
      "executor": {
        "path": "flip.nvflare.executors.RUN_VALIDATOR",
        "args": {
          "project_id": "{project_id}",
          "query": "{query}"
        }
      }
    }
  ],
  "task_result_filters": [
      {
      "tasks": ["train"],
      "filters": [
        {
        "id": "percentile_privacy",
        "path": "flip.nvflare.components.PercentilePrivacy",
        "args": {
          "gamma": 2.0,
          "percentile": 95,
          "off": false
      }
        }
      ]
    }
  ],
  "task_data_filters": [],
  "components": [
    {
      "id": "flip_client_event_handler",
      "path": "flip.nvflare.components.ClientEventHandler",
      "args": {}
    }
  ]
}
